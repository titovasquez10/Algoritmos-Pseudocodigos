Algoritmo num_mayor
	
	Definir a, b Como Entero;
	
	Escribir ("Digite un numero: ");
	Leer a;
	
	Escribir ("Digite otro numero: ");
	Leer b;
	
	Si a > b Entonces
		Escribir ("Es mayor");
		Escribir a;
	FinSi
	
	Si b > a Entonces
		Escribir ("Es mayor");
		Escribir b;
	FinSi
	
	Si a = b Entonces
		Escribir "Son iguales"," " a," & ", b;
	FinSi
	
	
FinAlgoritmo
