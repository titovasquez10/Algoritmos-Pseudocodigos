Algoritmo expresion_Algebraica
	
	Definir z, x, a, b, Valor_De_La_Expresion Como Real;
	
	Escribir ("Ingresar Valor Z: ");
	Leer z;
	
	Escribir ("Ingresar Valor X: ");
	Leer x;
	
	a = 3;
	b = 2;
	
	Valor_De_La_Expresion <- 3*a - 8* x *(b^3 - 5*z) + x/z;
	
	Escribir (Valor_De_La_Expresion) 
	
	
	
	
	
FinAlgoritmo
